rdization\n","print(\"\\nAFTER STANDARDIZATION:\")\n","print(f\"Unique values in 'Sex' column: {df_clean['Sex'].unique()}\")\n","print(f\"Data type of 'Fare' column: {df_clean['Fare'].dtype}\")\n","print(f\"Sample 'Fare' values: {df_clean['Fare'].head(5).tolist()}\")\n","\n","# Validate the standardization\n","sex_standardized = all(df_clean['Sex'].isin(['male', 'female']))\n","fare_decimals = all(df_clean['Fare'].apply(lambda x: len(str(x).split('.')[-1]) <= 2 if '.' in str(x) else True))\n","\n","print(f\"\\nVALIDATION:\")\n","print(f\"All 'Sex' values standardized to lowercase: {sex_standardized}\")\n","print(f\"All 'Fare' values have max 2 decimal places: {fare_decimals}\")\n","\n","# Display the standardized DataFrame\n","print(\"\\nStandardized DataFrame (first 10 rows):\")\n","display(df_clean.head(10))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"j5qy7tjoV86r","executionInfo":{"status":"ok","timestamp":1758387095941,"user_tz":-180,"elapsed":295,"user":{"displayName":"ÿ≥ÿßÿ±Ÿá ÿ£ÿ≠ŸÖÿØ ÿπŸàÿØÿ© ÿßÿ®Ÿà ŸÖŸÜÿØŸäŸÑ","userId":"06415661594324235524"}},"outputId":"1456cc83-efb5-41df-c5f8-2342bad497f6"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["=== TASK 3: STANDARDIZE FORMATS ===\n","BEFORE STANDARDIZATION:\n","Unique values in 'Sex' column: ['male' 'female']\n","Data type of 'Fare' column: float64\n","Sample 'Fare' values: [7.25, 71.2833, 7.925, 53.1, 8.05]\n","\n","AFTER STANDARDIZATION:\n","Unique values in 'Sex' column: ['male' 'female']\n","Data type of 'Fare' column: float64\n","Sample 'Fare' values: [7.25, 71.28, 7.92, 53.1, 8.05]\n","\n","VALIDATION:\n","All 'Sex' values standardized to lowercase: True\n","All 'Fare' values have max 2 decimal places: True\n","\n","Standardized DataFrame (first 10 rows):\n"]},{"output_type":"display_data","data":{"text/plain":["   PassengerId  Survived  Pclass  \\\n","0            1         0       3   \n","1            2         1       1   \n","2  